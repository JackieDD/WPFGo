<Window x:Class="DataProvider.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataProvider"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="200">
    <Grid>
        <StackPanel>
            <TextBox Name="addParam1" Margin="5" Height="20"/>
            <TextBox Name="addParam2" Margin="5" Height="20"/>
            <TextBox Name="addResult" Margin="5" Height="20" IsEnabled="False"/>
            <Button Content="ObjectDataProvider"  Margin="5" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
