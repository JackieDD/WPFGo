<Window x:Class="BindingGo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BindingGo"
        mc:Ignorable="d"
        Title="MainWindow" Height="110" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Name="textBoxName" BorderBrush="Black" Margin="5" Style="{DynamicResource RoundCornerTextBoxStyle}"/>
            <Button Content="Add Age" Margin="5" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
