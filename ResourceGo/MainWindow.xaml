        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:ResourceGo"
        mc:Ignorable="d"
        Title="MainWindow" FontSize="16">
    <Window.Resources>
       
            <!--<sys:String x:Key="str">
                沉舟侧畔千帆过
            </sys:String>-->
            <sys:Double x:Key="dbl">
                3.1415926
