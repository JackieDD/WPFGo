<Window x:Class="DependenceProperty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DependenceProperty"
        mc:Ignorable="d"
        Title="MainWindow" Height="160" Width="260">
    <Grid>
        <StackPanel>
            <TextBox Name="textBox1" BorderBrush="Black" Margin="5"/>
            <TextBox Name="textBox2" BorderBrush="Black" Margin="5"/>
            <Button Content="OK" Margin="5" Click="Button_Click"/>
            <Button Content="AttachProperty" Margin="5" Click="Button_Click_1" />
        </StackPanel>
    </Grid>
</Window>
